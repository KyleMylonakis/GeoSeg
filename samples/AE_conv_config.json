{
  "model": {
    "meta_arch": {
      "name": "AE",
      "compression": 2,
      "num_classes": 2,
      "num_layers": 2,
      "final_layer": {
        "filters": 2,
        "kernel_size": [
          1,
          1
        ],
        "strides": [
          1,
          3
        ],
        "padding": "same",
        "activation": "softmax",
        "name": "softmax"
      }
    },
    "block": {
      "name": "conv",
      "base": {
        "kernel_size": 3,
        "dropout": 0.5,
        "bottleneck": true,
        "bottleneck_factor": 4,
        "activation": "relu"
      },
      "transition": {
        "kernel_size": 2,
        "compression": 2,
        "activation": "relu"
      },
      "batch_norm": false,
      "filters": 4
    }
  },
  "train": {
    "data": "fga_data_set/train/data_bucket_1.npy",
    "labels": "fga_data_set/train/label_bucket_1.npy",
    "downsample": 25,
    "optimizer": {
      "algorithm": "nadam",
      "parameters": {
        "lr": 0.002
      }
    },
    "batch_size": 512,
    "epochs": 1000,
    "shuffle": true,
    "save_every" : 100
  },
  "eval": {
    "data": "fga_data_set/eval/data_bucket_2.npy",
    "labels": "fga_data_set/eval/label_bucket_2.npy",
    "batch_size": 32,
    "shuffle": false
  }
}