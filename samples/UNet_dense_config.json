{
    "model": {
      "num_classes": 2,
      "meta_arch": {
        "name": "UNet",
        "init_filters": 4,
        "compression": 2,
        "num_classes": 2,
        "num_rungs": 2,
        "first_layer": {
            "padding": "same",
            "activation": "relu",
            "kernel_size": [
              3,
              1
            ],
            "name": "first_layer"
          },
        "final_layer": {
          "name": "softmax",
          "activation": "softmax",
          "padding": "same",
          "strides": [
            1,
            3
          ],
          "kernel_size": [
            1,
            1
          ]
        }},
      "block": {
        "name": "dense",
        "base": {
          "kernel_size": 3,
          "dropout": 0.5,
          "num_layers": 4,
          "bottleneck": true,
          "activation": "relu"
        },
        "transition": {
          "kernel_size": 2,
          "compression": 2,
          "activation": "relu"
        },
        "batch_norm": false
      }
    },
    "train": {
        "data":"fga_data_set/train/data_bucket_1.npy",
        "labels":"fga_data_set/train/label_bucket_1.npy",
        "downsample": 125,
        "optimizer":{
            "algorithm": "nadam",
            "parameters":{
            "lr": 0.002}
        },
        "batch_size": 512,
        "epochs": 100,
        "shuffle": true
    },
    "eval":{
        "data":"fga_data_set/eval/data_bucket_2.npy",
        "labels":"fga_data_set/eval/label_bucket_2.npy",
        "batch_size": 32,
        "shuffle": false
    }
  }