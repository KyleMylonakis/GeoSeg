{
  "model": {
    "meta_arch": {
      "name": "cnn",
      "num_receivers": 6,
      "compression": 2,
      "num_classes": 2,
      "num_layers": 2,
      "final_layer": {
        "name": "binary-1d",
        "filters": 4,
        "activation": "softmax"
      }
    },
    "block": {
      "name": "dense",
      "base": {
        "kernel_size": [
          3,
          1
        ],
        "dropout": 0.5,
        "bottleneck": true,
        "bottleneck_factor": 4,
        "activation": "relu",
        "num_layers": 4
      },
      "transition": {
        "kernel_size": [
          2,
          1
        ],
        "compression": 2,
        "activation": "relu"
      },
      "batch_norm": false,
      "filters": 4
    }
  },
  "train": {
    "data": "PATH/TO/DATA.npy",
    "labels": "PATH/TO/LABELS.npy",
    "downsample": 25,
    "optimizer": {
      "algorithm": "nadam",
      "parameters": {
        "lr": 0.002
      }
    },
    "batch_size": 8,
    "epochs": 1,
    "shuffle": true,
    "save_every": 1
  },
  "eval": {
    "data": "PATH/TO/DATA.npy",
    "labels": "PATH/TO/LABELS.npy",
    "batch_size": 8,
    "shuffle": false
  }
}